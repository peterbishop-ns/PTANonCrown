                        <VisualState.Setters>
                            <Setter Property="BackgroundColor"
                                    Value="#F6CEFC" />
                            <Setter Property="TextColor"
                                    Value="Black" />
                        </VisualState.Setters>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateGroupList>
        </Setter>
